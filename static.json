{
  "routes": [
    {
      "src": "/(images|static|favicon\\.ico|manifest\\.json|logo192\\.png|logo512\\.png)/.*",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      },
      "continue": true
    },
    {
      "src": "/.*",
      "dest": "/index.html"
    }
  ]
}
